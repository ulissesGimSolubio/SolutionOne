@model int

@{
    if (Model != 0)
    {
        <p><i class="fa-solid fa-filter"></i> Esta consulta retornou <b>@Model registro(s)</b> de um total de <b>@ViewData["totalRegistros"]</b</p>
    }
    else
    {
        <p><i class="fa-solid fa-filter"></i> <b>Esta consulta não resultou nenhum registro</b>.</p>
    }
}
